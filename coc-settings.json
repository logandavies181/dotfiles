{
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": [
        "serve"
      ],
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {},
      "settings": {}
    },
    "golang": {
      "command": "/home/vagrant/go/bin/gopls",
      "filetypes": [
        "go"
      ],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  },

  "coc.preferences.rootPatterns": [
    ".git",
    ".hg",
    ".projections.json",
    "go.mod"
  ]
}
